.PHONY: clean All

All:
	@echo "----------Building project:[ week_2_opdracht_3 -  ]----------"
	@cd "week_2_opdracht_3" && "$(MAKE)" -f  "week_2_opdracht_3.mk"
clean:
	@echo "----------Cleaning project:[ week_2_opdracht_3 -  ]----------"
	@cd "week_2_opdracht_3" && "$(MAKE)" -f  "week_2_opdracht_3.mk" clean
