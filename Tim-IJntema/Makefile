.PHONY: clean All

All:
	@echo "----------Building project:[ opdracht_2_week_1 - Debug ]----------"
	@cd "opdracht_2_week_1" && "$(MAKE)" -f  "opdracht_2_week_1.mk"
clean:
	@echo "----------Cleaning project:[ opdracht_2_week_1 - Debug ]----------"
	@cd "opdracht_2_week_1" && "$(MAKE)" -f  "opdracht_2_week_1.mk" clean
