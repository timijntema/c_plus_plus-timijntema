.PHONY: clean All

All:
	@echo "----------Building project:[ week_4_opdracht_6 -  ]----------"
	@cd "week_4_opdracht_6" && "$(MAKE)" -f  "week_4_opdracht_6.mk"
clean:
	@echo "----------Cleaning project:[ week_4_opdracht_6 -  ]----------"
	@cd "week_4_opdracht_6" && "$(MAKE)" -f  "week_4_opdracht_6.mk" clean
